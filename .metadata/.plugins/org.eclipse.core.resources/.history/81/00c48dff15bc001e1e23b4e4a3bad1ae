<web-app>
	<servlet>
		<servlet-name>HelloServletMapping</servlet-name>
		<servlet-class>web_basic.HelloServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>UseRequestServlet1Mapping</servlet-name>
	<servlet-class>ch11.UseRequestServlet1</servlet-class>
	</servlet> 
	
	<servlet-mapping>
		<servlet-name>HelloServletMapping</servlet-name>
		<url-pattern>/HelloServlet</url-pattern>
		
	<servlet-mapping>
		<servlet-name>UseRequestServlet1Mapping</servlet-name>
		<url-pattern>/UseRequestServlet1</url-pattern>
	</servlet-mapping>
		
	</servlet-mapping>
</web-app>



